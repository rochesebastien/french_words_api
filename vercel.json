{
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@now/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "index.js"
      }
    ],
    "crons": [
      {
          "path": "/day/word/update",
          "schedule": "7 20 * * *"
      },
      {
          "path": "/day/suite/update",
          "schedule": "7 20 * * *"
      }
  ]
  }